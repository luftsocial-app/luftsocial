{
  "name": "nestjs-startup-template",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "eslint:fix": "eslint --fix \"{src,apps,libs,test}/**/*.ts\""
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.0",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.7",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.7",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^11.0.3",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "config": "^3.3.12",
    "http2": "^3.3.7",
    "joi": "^17.13.3",
    "js-yaml": "^4.1.0",
    "mailparser-mit": "^1.0.0",
    "nestjs-pino": "^4.3.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.1",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sanitize-html": "^2.14.0",
    "simple-oauth2": "^5.1.0",
    "swagger-ui-express": "^5.0.1",
    "typed-rest-client": "^2.1.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@jest-mock/express": "^2.1.0",
    "@nestjs/cli": "^11.0.2",
    "@nestjs/common": "^11.0.7",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.7",
    "@testcontainers/postgresql": "^10.17.2",
    "@types/chance": "^1.1.6",
    "@types/config": "^3.3.5",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.12.0",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/sanitize-html": "^2.13.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "chance": "^1.1.12",
    "cspell": "^8.17.3",
    "depcheck": "^1.4.7",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "nock": "^14.0.0",
    "pino-pretty": "^13.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "testcontainers": "^10.17.2",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "clearMocks": true,
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/",
      "src/app.module.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "./tsconfig.json",
        "diagnostics": false,
        "isolatedModules": true
      }
    },
    "testTimeout": 25000,
    "displayName": {
      "name": "Default name",
      "color": "blue"
    }
  },
  "pre-commit": [
    "eslint --fix",
    "prettier -c \"src/**/*.ts\" \"test/**/*.ts\"",
    "cspell '**/*.{js,ts,json,md,yml,yaml}'",
    "depcheck",
    "test",
    "build"
  ]
}