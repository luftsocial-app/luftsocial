  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "eslint:fix": "eslint --fix \"{src,apps,libs,test}/**/*.ts\"",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate -d src/database/migrations/config/typeorm.config.ts  src/database/migrations/luftsocialDB/migrations",
    "migration:create": "npm run typeorm -- migration:create src/database/migrations/updates/${NewMigrationFileName}",
    "migration:run": "npm run typeorm -- migration:run -d src/database/migrations/config/typeorm.config.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/database/migrations/config/typeorm.config.ts"
  },
